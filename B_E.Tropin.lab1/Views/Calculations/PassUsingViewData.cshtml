@{
    ViewData["Title"] = "PassUsingViewData";
}

<h1>PassUsingViewData</h1>

<b>Rand Value First : @ViewData["val1"]</b><br>
<b>Rand Value Second : @ViewData["val2"]</b><br>
<b>Add :</b>
<p class="m-0">@ViewData["val1"] + @ViewData["val2"] = @ViewData["add"]</p>
<b>Sub :</b>
<p class="m-0">@ViewData["val1"] - @ViewData["val2"] = @ViewData["sub"]</p>
<b>Mult :</b>
<p class="m-0">@ViewData["val1"] * @ViewData["val2"] = @ViewData["mult"]</p>
<b>Div :</b>
<p class="m-0">@ViewData["val1"] / @ViewData["val2"] = @ViewData["div"]</p>
