@{
    ViewData["Title"] = "PassUsingModel";
}

 <h1>PassUsingModel</h1>

<b>Rand Value First : @Model.val1</b><br>
<b>Rand Value Second : @Model.val2</b><br>
<b>Add :</b>
<p class="m-0">@Model.val1 + @Model.val2 = @Model.add</p>
<b>Sub :</b>
<p class="m-0">@Model.val1 - @Model.val2 = @Model.sub</p>
<b>Mult :</b>
<p class="m-0">@Model.val1 * @Model.val2 = @Model.mult</p>
<b>Div :</b>
<p class="m-0">@Model.val1 / @Model.val2 = @Model.div</p>
